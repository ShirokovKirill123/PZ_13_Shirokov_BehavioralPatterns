<Window x:Class="_2_ChainOfResponsibility_Resources.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_2_ChainOfResponsibility_Resources"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid  Margin="10">
        <StackPanel Orientation="Vertical" Margin="100">
            <TextBlock x:Name= "PasswordText" Text="Введите роль (Admin, Manager, User)" Width="300" Margin="0,0,0,10"  />
            <TextBox x:Name="RoleTextBox" Width="300" Margin="0,0,0,10" />
            <Button x:Name="SelectFileButton" Width="300" Content="Выбрать файл" Click="SelectFileButton_Click"/>
            <TextBlock x:Name="ResultTextBlock" TextWrapping="Wrap" Margin="0,50,0,0" Width="376"/>
        </StackPanel>
    </Grid>
</Window>
